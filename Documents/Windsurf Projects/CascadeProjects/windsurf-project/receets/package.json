{
  "name": "receets",
  "version": "1.0.0",
  "description": "POS-integrated checkout and returns system with real-time synchronization",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "admin": "cd admin && npm start",
    "install:all": "npm install && cd client && npm install && cd ../admin && npm install",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\" \"npm run admin\"",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "socket.io": "^4.7.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "stripe": "^13.5.0",
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "concurrently": "^8.2.1"
  }
}
